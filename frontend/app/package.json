{
  "name": "finanze",
  "version": "0.7.0",
  "description": "A personal portfolio tracker app that helps you dentralize a variety of assets and investments across different financial entites.",
  "main": "dist-electron/main/index.js",
  "packageManager": "pnpm@10.28.0",
  "keywords": [],
  "author": "marcosav",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:mobile": "pnpm run build:python && vite --host --mode mobile",
    "build": "tsc && vite build",
    "build:python": "node scripts/bundle-python.js",
    "build:mobile": "MOBILE_BUILD=1 pnpm run build && pnpm run build:python",
    "install:pyodide": "node scripts/sync-python-wheels.mjs && node scripts/sync-pyodide.mjs",
    "clean": "node scripts/clean-offline.mjs",
    "preview": "vite preview",
    "dist": "vite build && pnpm exec electron-builder",
    "dist:win": "vite build && pnpm exec electron-builder --win",
    "dist:linux": "vite build && pnpm exec electron-builder --linux",
    "dist:mac:arm64": "vite build && pnpm exec electron-builder --mac --arm64",
    "dist:mac:x64": "vite build && pnpm exec electron-builder --mac --x64",
    "lint": "eslint .",
    "format": "prettier --write .",
    "pretest": "vite build --mode=test",
    "test": "vitest run",
    "lint-staged": "lint-staged --no-stash",
    "cap:sync": "pnpm build:mobile && pnpm exec cap sync",
    "cap:ios": "pnpm build:mobile && pnpm exec cap sync ios && pnpm exec cap open ios",
    "cap:android": "pnpm build:mobile && pnpm exec cap sync android && pnpm exec cap open android"
  },
  "lint-staged": {
    "*": "eslint .",
    "*.*": "prettier --check ."
  },
  "dependencies": {
    "@capacitor-community/sqlite": "7.0.2-dev.5a79381.1766041651",
    "@capacitor/android": "^8.0.1",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/device": "^8.0.0",
    "@capacitor/filesystem": "^8.1.0",
    "@capacitor/ios": "^8.0.1",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capgo/capacitor-native-biometric": "^8.3.1",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.91.1",
    "@tanstack/react-virtual": "^3.13.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.7.3",
    "electron-window-state": "^5.0.3",
    "framer-motion": "^12.29.0",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.563.0",
    "pyodide": "^0.29.2",
    "react-day-picker": "^9.13.0",
    "react-markdown": "^10.1.0",
    "recharts": "^2.15.4",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "electron-is-dev": "^3.0.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.1.0",
    "jeep-sqlite": "^2.8.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.13.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vite-plugin-electron": "^0.29.0",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "sql.js": "1.11.0"
    },
    "patchedDependencies": {
      "@capgo/capacitor-native-biometric@8.3.1": "patches/@capgo__capacitor-native-biometric@8.3.1.patch"
    },
    "ignoredBuiltDependencies": [
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller"
    ]
  }
}
